    "editor.matchBrackets": false,
    "java.semanticHighlighting.enabled": true,
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
    "terminal.integrated.automationShell.windows": "wt",
    "window.titleSeparator": " -> ",
    "explorer.openEditors.visible": 12,
    "terminal.external.linuxExec": "urxvt",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "glassit.alpha": 235,
    "C_Cpp.autocompleteAddParentheses": true,
    "settingsSync.ignoredSettings": [
        "-C_Cpp.clang_format_fallbackStyle"
    ],
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "RPC.showTime": false,
    "RPC.showProblems": false,
    "RPC.disconnectOnIdle": true,
    "workbench.preferredDarkColorTheme": "Tokyo City Darker",
    "workbench.preferredLightColorTheme": "Tokyo City",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.inlayHints.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "workbench.colorTheme": "Tokyo City",
    "window.zoomLevel": 1,
    "security.workspace.trust.enabled": false
}
